# Replace empty with 0
df[is.na(df$place)==TRUE, ]$place<-"NA"
# Inserting places
for (row in 1:nrow(df)) {
place <- df[row, "place"]
if(place!="NA") {
next
} else{
df[row,"place"]$place<-df[row-1,"place"]$place
}
}
